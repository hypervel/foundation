{
    "name": "laravel-hyperf/foundation",
    "description": "The Support package for Laravel Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "foundation",
        "swoole",
        "laravel-hyperf"
    ],
    "support": {
        "issues": "https://github.com/laravel-hyperf/components/issues",
        "source": "https://github.com/laravel-hyperf/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "nesbot/carbon": "^2.16",
        "laravel-hyperf/filesystem": "dev-main",
        "laravel-hyperf/support": "dev-main",
        "hyperf/config": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "hyperf/command": "~3.1.0",
        "hyperf/crontab": "~3.1.0",
        "hyperf/collection": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/http-server": "~3.1.0",
        "hyperf/stringable": "~3.1.0",
        "hyperf/dispatcher": "~3.1.0",
        "hyperf/database": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "hyperf/signal": "~3.1.0",
        "hyperf/engine": "^2.1",
        "hyperf/db-connection": "~3.1.0",
        "hyperf/framework": "~3.1.0",
        "friendsofhyperf/pretty-console": "~3.1.0",
        "friendsofhyperf/command-signals": "~3.1.0",
        "symfony/error-handler": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelHyperf\\Foundation\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "hyperf": {
            "config": "LaravelHyperf\\Foundation\\ConfigProvider",
            "providers": [
                "LaravelHyperf\\Foundation\\Providers\\FoundationServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
